{
  "name": "ivebot-dashboard",
  "version": "4.0-alpha.3",
  "description": "Web dashboard for IveBot.",
  "author": "Ibrahim Ansari <ansari.ibrahim1@gmail.com>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "url": "https://github.com/retrixe/IveBot.git",
    "directory": "dashboard"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next",
    "lint": "graphql-schema-linter && tsc && eslint --cache --cache-strategy content --cache-location .next/ .",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@apollo/server": "^4.12.1",
    "@as-integrations/next": "^3.2.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/material-nextjs": "^7.1.1",
    "@projectdysnomia/dysnomia": "^0.2.3",
    "deepmerge": "^4.3.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^6.17.0",
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@eslint/js": "^9.28.0",
    "@next/eslint-plugin-next": "^15.3.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "graphql-schema-linter": "^3.0.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "graphql-schema-linter": {
    "rules": [
      "arguments-have-descriptions",
      "defined-types-are-used",
      "deprecations-have-a-reason",
      "descriptions-are-capitalized",
      "enum-values-all-caps",
      "enum-values-have-descriptions",
      "enum-values-sorted-alphabetically",
      "fields-are-camel-cased",
      "fields-have-descriptions",
      "input-object-fields-sorted-alphabetically",
      "input-object-values-are-camel-cased",
      "input-object-values-have-descriptions",
      "interface-fields-sorted-alphabetically",
      "types-are-capitalized",
      "types-have-descriptions"
    ],
    "disabledRules": [
      "type-fields-sorted-alphabetically"
    ],
    "schemaPaths": [
      "schema.graphql"
    ]
  }
}
