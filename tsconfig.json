{
  "extends": "@tsconfig/node24/tsconfig.json",
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "lib/",
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "rewriteRelativeImportExtensions": true,
    "erasableSyntaxOnly": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "src/**/*.ts",
    "index.d.ts"
  ],
  "exclude": [
    "dashboard"
  ]
}
